<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT" follow="true">
			<!--<PatternLayout pattern="%d{DEFAULT} %5p [%t] (%F:%L) - %m%n" />-->
			<PatternLayout pattern="%d{ABSOLUTE} %5p [%t] (%F:%L) - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>
